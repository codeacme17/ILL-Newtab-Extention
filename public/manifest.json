{
  "name": "1ll Tab",
  "description": "It's to create a new tab like google tab",
  "version": "1.1.1",
  "manifest_version": 3,
  "permissions": ["tabCapture", "tabs", "activeTab"],
  "background": {},
  "chrome_url_overrides": {
    "newtab": "./index.html"
  },
  "icons": {
    "16": "./icon/google-16.png",
    "32": "./icon/google-32.png",
    "48": "./icon/google-48.png",
    "128": "./icon/google-128.png"
  }
}
